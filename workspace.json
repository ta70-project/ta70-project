    },
    "app-angular": {
      "projectType": "application",
      "root": "apps/app-angular",
      "sourceRoot": "apps/app-angular/src",
      "prefix": "app-angular",
      "targets": {
        "build": {
          "executor": "@angular-devkit/build-angular:browser",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/apps/app-angular",
            "index": "apps/app-angular/src/index.html",
            "main": "apps/app-angular/src/main.ts",
            "polyfills": "apps/app-angular/src/polyfills.ts",
            "tsConfig": "apps/app-angular/tsconfig.app.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              "apps/app-angular/src/favicon.ico",
              "apps/app-angular/src/assets",
              {
                "glob": "**",
                "input": "libs/assets/src/lib/",
                "output": "assets/"
              }
            ],
            "styles": ["apps/app-angular/src/styles.scss"],
            "scripts": []
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "4mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kb",
                  "maximumError": "4kb"
                }
              ],
              "fileReplacements": [
                {
                  "replace": "apps/app-angular/src/environments/environment.ts",
                  "with": "apps/app-angular/src/environments/environment.prod.ts"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "buildOptimizer": false,
              "optimization": false,
              "vendorChunk": true,
              "extractLicenses": false,
              "sourceMap": true,
              "namedChunks": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "executor": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "browserTarget": "app-angular:build:production"
            },
            "development": {
              "browserTarget": "app-angular:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "executor": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "app-angular:build"
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["apps/app-angular/src/**/*.ts", "apps/app-angular/src/**/*.html"]
          }
        }
      }
    },
    "app-electron": {
      "root": "apps/app-electron",
      "sourceRoot": "apps/app-electron/src",
      "projectType": "application",
      "prefix": "app-electron",
      "targets": {
        "build": {
          "executor": "nx-electron:build",
          "options": {
            "outputPath": "dist/apps/app-electron",
            "main": "apps/app-electron/src/main.ts",
            "tsConfig": "apps/app-electron/tsconfig.app.json",
            "assets": [
              "apps/app-electron/src/assets",
              {
                "glob": "**",
                "input": "libs/assets/src/lib/",
                "output": "assets/"
              }
            ]
          },
          "configurations": {
            "production": {
              "optimization": true,
              "extractLicenses": true,
              "inspect": false,
              "fileReplacements": [
                {
                  "replace": "apps/app-electron/src/environments/environment.ts",
                  "with": "apps/app-electron/src/environments/environment.prod.ts"
                }
              ]
            }
          }
        },
        "serve": {
          "executor": "nx-electron:execute",
          "options": {
            "buildTarget": "app-electron:build"
          }
        },
        "package": {
          "executor": "nx-electron:package",
          "options": {
            "name": "app-electron",
            "frontendProject": "app-angular",
            "outputPath": "dist/packages",
            "prepackageOnly": true,
            "makerOptionsPath": "apps/app-electron/maker.options.json"
          }
        },
        "make": {
          "executor": "nx-electron:make",
          "options": {
            "name": "app-electron",
            "frontendProject": "app-angular",
            "outputPath": "dist/executables",
            "makerOptionsPath": "apps/app-electron/maker.options.json"
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["apps/app-electron/**/*.ts"]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/apps/app-electron"],
          "options": {
            "jestConfig": "apps/app-electron/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    }
  },
  "cli": {
    "defaultCollection": "@nrwl/angular"
  },
  "generators": {
    "@nrwl/angular:application": {
      "style": "scss",
      "linter": "eslint",
      "unitTestRunner": "none",
      "e2eTestRunner": "none"
    },
    "@nrwl/angular:library": {
      "style": "scss",
      "linter": "eslint",
      "unitTestRunner": "none"
    },
    "@nrwl/angular:component": {
      "style": "scss"
    }
  }
}
