{
  "name": "open-days-with-alexa",
  "version": "0.2.0",
  "license": "MIT",
  "scripts": {
    "nx": "nx",
    "postinstall": "exitzero electron-builder install-app-deps && ngcc --properties es2015 browser module main",
    "start": "run-p start:app-webservice start:app-desktop",
    "start:app-webservice": "nx serve app-webservice --configuration=development",
    "start:app-desktop": "run-p start:app-desktop-angular start:app-desktop-electron",
    "start:app-desktop-angular": "nx serve app-desktop-angular --configuration=development",
    "start:app-desktop-electron": "wait-on tcp:4200 && nx serve app-desktop-electron --configuration=development",
    "publish:step1": "nx run-many --target=build --configuration=production --projects=app-desktop-angular,app-desktop-electron --parallel=true",
    "publish:step2:windows": "nx run app-desktop-electron:make --configuration=production --arch x64 --platform windows",
    "publish:step2:macos": "nx run app-desktop-electron:make --configuration=production --arch x64 --platform mac",
    "publish:step2:linux:": "nx run app-desktop-electron:make --configuration=production --arch x64 --platform linux",
    "publish:windows": "npm-run-all publish:step1 publish:step2:windows",
    "publish:macos": "npm-run-all publish:step1 publish:step2:macos",
    "publish:linux": "npm-run-all publish:step1 publish:step2:linux"
  },
  "private": true,
