{
  "name": "open-days-with-alexa",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "postinstall": "exitzero electron-builder install-app-deps && ngcc --properties es2015 browser module main",
    "start": "run-p start:step1 start:step2",
    "nx": "nx",
    "start:step1": "nx serve app-desktop-angular",
    "start:step2": "wait-on tcp:4200 && nx serve app-desktop-electron",
    "publish:step1": "nx run-many --target=build --configuration=production --projects=app-desktop-angular,app-desktop-electron --parallel=true",
    "publish:step2:windows": "nx run app-desktop-electron:make --arch x64 --platform windows",
    "publish:step2:macos": "nx run app-desktop-electron:make --arch x64 --platform mac",
    "publish:step2:linux:": "nx run app-desktop-electron:make --arch x64 --platform linux",
    "publish:windows": "npm-run-all publish:step1 publish:step2:windows",
    "publish:macos": "npm-run-all publish:step1 publish:step2:macos",
    "publish:linux": "npm-run-all publish:step1 publish:step2:linux"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^12.2.8",
    "@angular/cdk": "^12.2.9",
    "@angular/common": "^12.2.0",
    "@angular/compiler": "^12.2.0",
    "@angular/core": "^12.2.0",
    "@angular/forms": "^12.2.0",
    "@angular/platform-browser": "^12.2.0",
    "@angular/platform-browser-dynamic": "^12.2.0",
    "@angular/router": "^12.2.0",
    "@azure/storage-blob": "^12.8.0",
    "@types/async": "^3.2.9",
    "@types/better-sqlite3": "^7.4.1",
    "@types/mssql": "^7.1.3",
    "@types/nodemailer": "^6.4.4",
    "@types/uuid": "^8.3.1",
    "async": "^3.2.2",
    "better-sqlite3": "^7.4.4",
    "cp-file": "^9.1.0",
    "fs-extra": "^10.0.0",
    "handlebars": "^4.7.7",
    "iconv-lite": "^0.6.3",
    "inversify": "^5.1.1",
    "mssql": "^4.0.4",
    "nodemailer": "^6.7.0",
    "primeflex": "^3.1.0",
    "primeicons": "^4.1.0",
    "primeng": "^12.2.2",
    "reflect-metadata": "^0.1.13",
    "rxjs": "~6.6.0",
    "toad-scheduler": "^1.6.0",
    "tslib": "^2.0.0",
    "typeorm": "^0.2.40",
    "uuid": "^8.3.2",
    "winston": "^3.3.3",
    "zone.js": "~0.11.4"
  },
